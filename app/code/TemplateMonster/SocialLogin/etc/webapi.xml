<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
    <route url="/V1/social/mage2_access_token/from/facebook/:fb_access_token" method="GET">
        <service class="TemplateMonster\SocialLogin\Api\SocialLoginTokenProviderInterface" method="getTokenforFacebook"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    
    <route url="/V1/social/mage2_access_token/from/google/:google_access_token" method="GET">
        <service class="TemplateMonster\SocialLogin\Api\SocialLoginTokenProviderInterface" method="getTokenforGoogle"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
   
</routes>
