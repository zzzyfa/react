<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/althea/products" method="GET">
        <service class="Althea\Catalog\Api\ProductRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="Magento_Catalog::products"/>
        </resources>
    </route>
    <route url="/V1/althea/categories" method="GET">
        <service class="Althea\Catalog\Api\CategoryManagementInterface" method="getTree"/>
        <resources>
            <resource ref="Magento_Catalog::categories"/>
        </resources>
    </route>
</routes>