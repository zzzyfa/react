    <body>
        <referenceBlock name="head.additional">
            <block class="Althea\CurrencyManager\Block\Js" name="althea_currencymanager.js.config">
                <action ifconfig="currencymanager/general/enabled" method="setTemplate">
                    <argument name="template" xsi:type="string">Althea_CurrencyManager::js.phtml</argument>
                </action>
            </block>
        </referenceBlock>
    </body>
</page>