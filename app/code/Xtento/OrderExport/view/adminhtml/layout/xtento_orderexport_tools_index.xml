<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\OrderExport\Block\Adminhtml\Widget\Menu" name="orderexport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\OrderExport\Block\Adminhtml\Tools" name="adminhtml.orderexport.tools">
                <block class="Xtento\OrderExport\Block\Adminhtml\Tools\Import" name="adminhtml.orderexport.tools.import" template="Xtento_OrderExport::tools/import.phtml"/>
                <block class="Xtento\OrderExport\Block\Adminhtml\Tools\Export" name="adminhtml.orderexport.tools.export" template="Xtento_OrderExport::tools/export.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>

