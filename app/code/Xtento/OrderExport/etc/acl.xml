<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Xtento_XtCore::extensions">
                        <resource id="Xtento_OrderExport::configuration" title="Magento Order Export Module" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Sales::sales">
                    <resource id="Xtento_OrderExport::menu" title="Magento Order Export Module" sortOrder="1000">
                        <resource id="Xtento_OrderExport::manual" title="Manual Export" sortOrder="5"/>
                        <resource id="Xtento_OrderExport::log" title="Execution Log" sortOrder="10"/>
                        <resource id="Xtento_OrderExport::history" title="Export History" sortOrder="15"/>
                        <resource id="Xtento_OrderExport::profile" title="Export Profiles" sortOrder="20"/>
                        <resource id="Xtento_OrderExport::destination" title="Export Destinations" sortOrder="30"/>
                        <resource id="Xtento_OrderExport::tools" title="Tools" sortOrder="40"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>

