<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Xtento_XtCore::extensions">
                        <resource id="Xtento_TrackingImport::configuration" title="Tracking Number Import Module" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Sales::sales">
                    <resource id="Xtento_TrackingImport::menu" title="Tracking Number Import Module" sortOrder="1000">
                        <resource id="Xtento_TrackingImport::manual" title="Manual Import" sortOrder="5"/>
                        <resource id="Xtento_TrackingImport::log" title="Execution Log" sortOrder="10"/>
                        <resource id="Xtento_TrackingImport::profile" title="Import Profiles" sortOrder="20"/>
                        <resource id="Xtento_TrackingImport::source" title="Import Sources" sortOrder="30"/>
                        <resource id="Xtento_TrackingImport::tools" title="Tools" sortOrder="40"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
