<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\TrackingImport\Block\Adminhtml\Widget\Menu" name="trackingimport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\TrackingImport\Block\Adminhtml\Source\Edit" name="trackingimport_source_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Xtento\TrackingImport\Block\Adminhtml\Source\Edit\Tabs" name="trackingimport_source_edit_tabs">
                <action method="addTab">
                    <argument name="name" xsi:type="string">general</argument>
                    <argument name="block" xsi:type="string">Xtento\TrackingImport\Block\Adminhtml\Source\Edit\Tab\Configuration</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>

