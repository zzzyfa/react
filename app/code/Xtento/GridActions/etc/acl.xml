<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Sales::sales">
                    <resource id="Magento_Sales::sales_operation">
                        <resource id="Magento_Sales::sales_order">
                            <resource id="Xtento_GridActions::actions" title="Simplify Bulk Order Processing Actions" sortOrder="100">
                                <resource id="Xtento_GridActions::invoice" title="Invoice" sortOrder="10"/>
                                <resource id="Xtento_GridActions::capture" title="Capture Payment" sortOrder="20"/>
                                <resource id="Xtento_GridActions::ship" title="Ship" sortOrder="30"/>
                                <resource id="Xtento_GridActions::print" title="Print" sortOrder="40"/>
                                <resource id="Xtento_GridActions::complete" title="Complete" sortOrder="50"/>
                                <resource id="Xtento_GridActions::email" title="Email" sortOrder="60"/>
                                <resource id="Xtento_GridActions::changestatus" title="Change Status" sortOrder="70"/>
                                <resource id="Xtento_GridActions::delete" title="Delete" sortOrder="80"/>
                            </resource>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::system">
                    <resource id="Xtento_XtCore::extensions">
                        <resource id="Xtento_GridActions::configuration" title="Simplify Bulk Order Processing" sortOrder="30"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
