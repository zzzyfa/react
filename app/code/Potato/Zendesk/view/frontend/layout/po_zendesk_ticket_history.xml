<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Potato\Zendesk\Block\Ticket\History" name="po_zendesk.ticket.history" ifconfig="potato_zendesk/features/frontend_customer_section">
                <container name="po_zendesk.ticket.history.info" as="info" label="Ticket History Info"/>
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="account/link/back.phtml" ifconfig="potato_zendesk/features/frontend_customer_section"/>
        </referenceContainer>
    </body>
</page>
